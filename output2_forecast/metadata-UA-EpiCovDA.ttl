covid-research:UA-EpiCovDA a covid:Research ;
    rdfs:label "UA-EpiCovDA" ;
    ns1:madeBySensor covid-model:UA-EpiCovDA ;
    ns1:usedProcedure covid-method:EpiCovDA .

covid-method:EpiCovDA a covid:Method ;
    rdfs:label "EpiCovDA" .

covid-model:UA-EpiCovDA a covid:Model ;
    rdfs:label "UA-EpiCovDA" ;
    covid:hasLicense covid-license:cc-by-4.0 ;
    covid:hasName "EpiGro Covid-19 model with Data Assimilation" ;
    covid:hasWebsite <https://jocelinelega.github.io/EpiGro/> ;
    covid:modelBrief "'SIR mechanistic model with data assimilation.  '" ;
    covid:modelDesc "'EpiCovDA is an extension of the EpiGro model. Model parameters are  fit to Covid-19 data using a variational data assimilation method. A prior distribution  of the parameters is estimated by fitting an SIR Incidence-Cumulative Cases curve  to data from states that had at least 1000 cases by 04/01/2020. See <https://jocelinelega.github.io/EpiGro/>  for more information about EpiGro. '" ;
    covid:modelDesignation covid-model-designation:primary ;
    covid:owner covid-owner:University_of_Arizona .

covid-owner:University_of_Arizona a covid:Team ;
    rdfs:label "University of Arizona" .

