@prefix ns1: <http://www.w3.org/ns/sosa/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

covid-research:UMass-ExpertCrowd a covid:Research ;
    rdfs:label "UMass-ExpertCrowd" ;
    ns1:madeBySensor covid-model:UMass-ExpertCrowd ;
    ns1:usedProcedure covid-method:ExpertCrowd .

covid-method:ExpertCrowd a covid:Method ;
    rdfs:label "ExpertCrowd" .

covid-model:UMass-ExpertCrowd a covid:Model ;
    rdfs:label "UMass-ExpertCrowd" ;
    covid:hasLicense covid-license:other ;
    covid:hasName "Expert consensus distributions" ;
    covid:hasWebsite <https://github.com/tomcm39/COVID19_expert_survey> ;
    covid:modelBrief "'A consensus forecast from experts in the modeling of infectious disease,  public health, and epidemiology. '" ;
    covid:modelDesc "'April 22: Experts in the modeling of infectious disease, public health,  and epidemiology provide forecasts of  the smallest, most likely, and largest possible  cumulative number of deaths at the US level. We transform each  expert''s three  estimates into a triangular probability distribution and take an equally-weighted  average of  expert predictive densities.'" ;
    covid:modelDesignation covid-model-designation:other ;
    covid:owner covid-owner:UMassAmherst_Influenza_Forecasting_Center_of_Excellence .

covid-owner:UMassAmherst_Influenza_Forecasting_Center_of_Excellence a covid:Team ;
    rdfs:label "UMass-Amherst Influenza Forecasting Center of Excellence" ;
    covid:fundingSource covid-funding-resource:CDC .

