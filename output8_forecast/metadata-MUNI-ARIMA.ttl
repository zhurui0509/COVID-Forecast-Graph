covid-research:MUNI-ARIMA a covid:Research ;
    rdfs:label "MUNI-ARIMA" ;
    ns1:madeBySensor covid-model:MUNI-ARIMA ;
    ns1:usedProcedure covid-method:ARIMA .

covid-method:ARIMA a covid:Method ;
    rdfs:label "ARIMA" .

covid-model:MUNI-ARIMA a covid:Model ;
    rdfs:label "MUNI-ARIMA" ;
    covid:hasLicense covid-license:cc-by-4.0 ;
    covid:hasName "ARIMA" ;
    covid:hasWebsite <https://sites.google.com/mail.muni.cz/covid19stat> ;
    covid:modelBrief "Seasonal ARIMA model with outlier detection fitted to transformed daily series. Weekly forecasts are obtained by aggregating bootstrap daily forecast paths." ;
    covid:modelDesc "This is a seasonal ARIMA model with outlier detection fitted to transformed daily series. Weekly forecasts are obtained by aggregating bootstrap daily forecast paths." ;
    covid:modelDesignation covid-model-designation:primary ;
    covid:owner covid-owner:Masaryk_University .

covid-owner:Masaryk_University a covid:Team ;
    rdfs:label "Masaryk University" .

